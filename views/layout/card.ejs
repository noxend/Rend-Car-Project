<!-- eslint-disable css-rparentexpected-->
<% data.forEach(function(items){ %>
<div class='col-xl-4 col-lg-4 col-md-6 col-sm-12 cardContent'>
    <div class='card1'>
        <a href="/info/<%-items.id%>"><div class='dark'></div></a>
        <div class='thumbnail' style="background-image: url(<%-items.image%>)"></div>
        <div class='data'>
            <h5>
                <%-items.mark_name%> <%-items.model%>
            </h5>
            <p>
                <%-items.year%>
            </p>
            <p>
                <%-items.country%>
            </p>
        </div>
        <div class='blur'></div>
        <div class='bottom1'>
            <nav class="nav nav-pills nav-justified">
                <a class="nav-link" href="/info/<%-items.id%>">Більше</a>
                <a class="nav-link" href="/info/<%-items.id%>">Орендувати</a>
            </nav>
        </div>
    </div>
</div>
<% }); %>